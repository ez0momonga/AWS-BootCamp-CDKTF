# AWS Workshop Environment Configuration
# Copy this file to .env and set your unique identifier

# IMPORTANT: Set your unique identifier here to avoid resource conflicts
# Examples: your name, student ID, or any unique string
# ENVIRONMENT_IDENTIFIER=your-name
# ENVIRONMENT_IDENTIFIER=student01
# ENVIRONMENT_IDENTIFIER=yamada-taro
ENVIRONMENT_IDENTIFIER=default

# AWS Configuration
AWS_DEFAULT_REGION=ap-northeast-1

# NOTE: Each student should use a different ENVIRONMENT_IDENTIFIER
# to avoid resource name conflicts in the shared AWS account